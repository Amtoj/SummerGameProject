*.psd filter=lfs diff=lfs merge=lfs -text # Photoshop files  
*.png filter=lfs diff=lfs merge=lfs -text # PNG images  
*.jpg filter=lfs diff=lfs merge=lfs -text # JPEG images  
*.tga filter=lfs diff=lfs merge=lfs -text # TGA textures  
*.tif filter=lfs diff=lfs merge=lfs -text # TIFF images 

*.fbx filter=lfs diff=lfs merge=lfs -text # 3D models  
*.obj filter=lfs diff=lfs merge=lfs -text # 3D object files  
*.blend filter=lfs diff=lfs merge=lfs -text # Blender files  
*.max filter=lfs diff=lfs merge=lfs -text # 3ds Max files  
*.ma filter=lfs diff=lfs merge=lfs -text # Maya ASCII  
*.mb filter=lfs diff=lfs merge=lfs -text # Maya Binary  

*.mp3 filter=lfs diff=lfs merge=lfs -text # MP3 audio  
*.wav filter=lfs diff=lfs merge=lfs -text # WAV files  
*.ogg filter=lfs diff=lfs merge=lfs -text # Ogg Vorbis  
*.flac filter=lfs diff=lfs merge=lfs -text # FLAC audio  

*.mp4 filter=lfs diff=lfs merge=lfs -text # MP4 video  
*.mov filter=lfs diff=lfs merge=lfs -text # MOV video  
*.avi filter=lfs diff=lfs merge=lfs -text # AVI video  

*.zip filter=lfs diff=lfs merge=lfs -text # ZIP archives  
*.rar filter=lfs diff=lfs merge=lfs -text # RAR archives  
*.7z filter=lfs diff=lfs merge=lfs -text # 7-Zip archives  
